<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <gradient
                android:angle="315"
                android:centerColor="#66aaaaaa"
                android:endColor="#FEFFFFFF"
                android:startColor="#88000000"
                android:type="linear" />
            <stroke
                android:width="1dp"
                android:color="#33000000" />
            <corners android:radius="3dp" />

            <padding
                android:bottom="1dp"
                android:left="2dp"
                android:right="1dp"
                android:top="2dp" />

        </shape>
    </item>
    <item>

        <shape>
            <solid android:color="#EEEEEEEE" />
            <!--<stroke android:width="1dp" android:color=" @color/primaryDark" />-->
            <corners android:radius="3dp" />

            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp" />

        </shape>
    </item>


</layer-list>